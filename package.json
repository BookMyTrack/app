{
  "name": "bookmytrack-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:remix": "remix build",
    "build:css": "npm run generate:css -- --minify",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "NODE_ENV=development binode --require ./mock -- @remix-run/dev:remix dev",
    "deploy": "fly deploy --remote-only",
    "start": "remix-serve build",
    "generate:css": "tailwindcss -i ./app/styles/tailwind.input.css -o ./app/styles/tailwind.css",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@radix-ui/colors": "^0.1.8",
    "@remix-run/node": "^1.6.1",
    "@remix-run/react": "^1.6.1",
    "@remix-run/serve": "^1.6.1",
    "@remix-run/vercel": "^1.6.7",
    "@vechaiui/core": "^0.6.0",
    "@vechaiui/react": "^0.2.2",
    "ariakit": "2.0.0-next.29",
    "ariakit-utils": "0.17.0-next.20",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "fp-ts": "^2.12.1",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "lodash.groupby": "^4.6.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^1.3.0",
    "tailwind-radix-colors": "^0.0.9",
    "ts-pattern": "^4.0.3",
    "zustand": "4.0.0-rc.1"
  },
  "devDependencies": {
    "@rawnly/eslint-config-ts": "^0.0.2",
    "@remix-run/dev": "^1.6.1",
    "@remix-run/eslint-config": "^1.6.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@types/lodash.groupby": "^4.6.7",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jsdom": "^20.0.0",
    "msw": "^0.42.3",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.19.1"
  },
  "engines": {
    "node": ">=14"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
